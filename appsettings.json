{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JWT": {
    "SecretKey": "%JWT_SECRETKEY%",
    "Issuer": "%JWT_ISSUER%",
    "Audience": "%JWT_AUDIENCE%"
  },
  "EmailSettings": {
    "SmtpServer": "%EMAIL_SMTP_SERVER%",
    "Port": "%EMAIL_SMTP_PORT%",
    "SenderEmail": "%EMAIL_SENDER%",
    "SenderPassword": "%EMAIL_PASSWORD%",
    "SenderName": "%EMAIL_FROM_NAME%",
    "UseSsl": true
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=caserelay.database.windows.net;Database=caserelay;User ID=diidee;Password={your_password};TrustServerCertificate=True;"
  },
  "Cloudinary": {
    "CloudName": "%CLOUDINARY_CLOUD_NAME%",
    "ApiKey": "%CLOUDINARY_API_KEY%",
    "ApiSecret": "%CLOUDINARY_API_SECRET%"
  }
}
